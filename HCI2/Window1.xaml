        <Label x:Name="label1" Content="Kraj termina" HorizontalAlignment="Left" Margin="20,82,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="Label" HorizontalAlignment="Left" Margin="129,82,0,0" VerticalAlignment="Top"/>
